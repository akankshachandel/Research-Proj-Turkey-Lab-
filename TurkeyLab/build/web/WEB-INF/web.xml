<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginService</servlet-name>
        <servlet-class>turkeylab.LoginService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchService</servlet-name>
        <servlet-class>turkeylab.SearchService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadService</servlet-name>
        <servlet-class>turkeylab.UploadService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFile</servlet-name>
        <servlet-class>turkeylab.UploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>multipleSearchService</servlet-name>
        <servlet-class>turkeylab.multipleSearchService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadFile</servlet-name>
        <url-pattern>/UploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginService</servlet-name>
        <url-pattern>/LoginService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchService</servlet-name>
        <url-pattern>/SearchService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadService</servlet-name>
        <url-pattern>/UploadService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>multipleSearchService</servlet-name>
        <url-pattern>/multipleSearchService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
